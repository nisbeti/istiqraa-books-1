<div class="nass margin-top-10"> <p><span id="p1" class="anchor"></span><span class="c5">فسد:</span> فساد ١/ ١٦٤ فسق: فسوق ١/ ٨٤، فسق ١/ ١٥٣، لفاسقون ١/ ٢٢٣،<span class="c5"> فاسقون ٢/ ٢٥٤ فصح:</span> افصح ٢/ ١٠٤ فصل: المفصل ١/ ٧، تفصل ١/ ١٩٣، فصاله ٢/ ١٢٧، الفصل ٢/ ١٦٨،<span class="c5"> فصيلته ٢/ ٢٦٩ فصم:</span> انفصام ١/ ٧٩ فضض: انفضوا ١/ ١٠٧، ٢/ ٢٥٨،<span class="c5"> ينفضوا ٢/ ٢٥٩ فضو:</span> أفضى ١/ ١٢٠ فطر: فاطر ١/ ١، ١٨٧، ٣٣٦، فطور ١/ ١٨٧، ٢/ ٢٦١، فطركم ١/ ٣٨٢ يتفطرن ٢/ ١٢، ١٩٩،<span class="c5"> منفطر به ٢/ ٢٧٤ فقد:</span> فقد ١/ ٣٥ فقر: فاقرة ٢/ ٢٧٨ فقع: فاقع ١/ ٤٤ فكك: منفكين ٢/ ٣٠٦ فكه: فكهين ٢/ ٢٣١ فلح: المفلحون ١/ ٢٩،<span class="c5"> الفلاح ١/ ٢٩ فلق:</span> الفلق ٢/ ٣١٧ فلك: الفلك ١/ ١٠، ٦٢، ٢٨٨، ٣٤١، ٢/ ١٩٥،<span class="c5"> فلك ٢/ ٣٨ فلل:</span> فلولا ١/ ٢٧١،<span class="c5"> ٣٠٠ فند:</span> تفندون ١/ ٣١٨ فنن: أفنان ٢/ ٢٤٥ فوج: فوجا ٢/ ٩٦، فوج ٢/ ١٨٦،<span class="c5"> أفواجا ٢/ ٣١٥ فوز:</span> بمفازة ١/ ١١١،<span class="c5"> بمفازاتهم ٢/ ١٩١ فوق:</span> فوقهم ١/ ٧٢، فوق ١/ ٩٥،<span class="c5"> فواق ٢/ ١٧٩ فوم:</span> فومها ١/ ٤١ فىء: فاؤا ١/ ٧٣، الفيء ٢/ ٧٦، فاء ٢/ ٧٧، أفاء ٢/ ١٣٨،<span class="c5"> تفيء ٢/ ٢١٩ فيض:</span> أفضتم ١/ ٧١، تفيض ١/ ٢٦٧، تفيضون ١/ ٢٧٨<a href="#p1" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>