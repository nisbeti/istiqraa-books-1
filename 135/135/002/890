<div class="nass margin-top-10"> <p><span id="p1" class="anchor"></span><span class="c5">عدن:</span> عدن ١/ ٢٦٣ عدو: عدوا ١/ ٢٠٣، ٢٨١، بالعدوة ١/ ٢٤٦، يعدون ١/ ٢٣٠، ولا تعد ١/ ٣٩٨، العدى ٢/ ١١، العدى ٢/ ١١، عدوان ٢/ ١٠٢، عدوة ٢/ ١٠٣،<span class="c5"> العاديات ٢/ ٣٠٧ عذب:</span> العذاب ١/ ١٢٣، ٢/ ٦٣، عذب ٢/ ٧٧، يعذب ٢/ ٢٩٨،<span class="c5"> عذر:</span> المعذرون ١/ ٢٦٧،<span class="c5"> معاذيره ٢/ ٢٧٨ عرب:</span> الأعراب ١/ ٣٤٦،<span class="c5"> عربا ٢/ ٢٥١ عرج:</span> يعرجون ١/ ٣٤٦، العرجون ٢/ ١٦١،<span class="c5"> معارج ٢/ ٢٠٣ عرر:</span> المعتر ٢/ ٥١،<span class="c5"> معرة ٢/ ٢١٧ عرش:</span> معروشات ١/ ٢٠٧، يعرشون ١/ ٢٢٧، ٣٦٤،<span class="c5"> العرش ١/ ٣١٩ عرض:</span> عرضة ١/ ٧٣، عرضتم ١/ ٧٥، عرض ١/ ٢٣٢، عرض الدنيا ١/ ٢٥٠،<span class="c5"> أعرض ٢/ ١٩٨ عرف:</span> الأعراف ١/ ٦، ٢١٥، المعروف ١/ ٧٥، بالعرف ١/ ٢٣٦، عرفها ٢/ ٢١٤، لتعارفوا ٢/ ٢٢١،<span class="c5"> عرفا ٢/ ٢٨٨ عرم:</span> عرمة ٢/ ١٤٦ عرو: العروة ١/ ٧٩ عرى: العراء ٢/ ١٧٥،<span class="c5"> ٢٦٦ عزب:</span> يعزب ١/ ٢٧٨،<span class="c5"> ٢/ ١٤٢ عزر:</span> وعزرتموهم ١/ ١٥٦،<span class="c5"> تعزروه ٢/ ٢١٧ عزز:</span> العزة ١/ ١٤١، فعززنا ٢/ ١٥٨، عزنى ٢/ ١٨١، العزيز ٢/ ٢٠٩ العزى ٢/ ٢٣٦،<span class="c5"> عزين ٢/ ٢٧٠ عزم:</span> عزمت ١/ ١٠٧،<span class="c5"> عزم ٢/ ٢٠١ عسر:</span> العسير ١/ ٦٠، عسيرا ٢/ ٧٤، عسير ٢/ ٢٧٥،<span class="c5"> العسر ٢/ ٣٠٣ عسع:</span> عسعس ٢/ ٢٨٧ عسى: عسيتم ١/ ٧٧، عسى ١/ ١٣٤، ٢٢٥،<span class="c5"> ٢٥٤ عشر:</span> عاشروهن ١/ ١٢٠،<span class="c5"> معشار ٢/ ١٥٠ عشن:</span> العشنزر ١/ ٣٩٥ عشو: يعش ٢/ ٢٠٤<a href="#p1" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>