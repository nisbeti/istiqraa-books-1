<div class="nass margin-top-10"> <p><span id="p1" class="anchor"></span><span class="c5">عمران القصير:</span> ٢: ١٥ ح عمر بن الخطاب: ١: ٢١، ٢٢ ح، ٣٦ ح ٨٥، ١٩٥، ٢١٥ ح،<span class="c5"> ٢٨٢ ح عمر بن عبد العزيز:</span> ١: ٣٠٧،<span class="c5"> ٢:</span> ٢١٢ ح عمر بن عبد ود: ٢: ١٥٩ عمر بن هبيرة: ٢: ٢٨٧ أبو عمرو بن أمية: ١: ١٢١ أبو عمرو بن العلاء: ١: ١٤ ح، ٤٩، ٦٩، ٧٠، ٧٥ ح، ٨٤، ١٢٢، ١٢٣، ١٧٤ ح، ٢٠٣، ٢٧٤، ٢٨٧، ٢٩٣، ٣٠٥، ٣٠٦، ٣٢٩، ٣٥٢، ٣٧٣،<span class="c5"> ٢:</span> ٢١، ٨٣، ١١٨، ١٣٧ ح، ١٧٣، ١٧٨، ١٨٨، ٢٠٧، ٢١٧، ٢٤٣، ٢٥٩، ٢٦١،<span class="c5"> ٢٦٩ أبو عمرو الهذلي:</span> ١: ٢٩، ١٠١، ١٣٨، ٣١٩،<span class="c5"> ٢:</span> ٣٤،<span class="c5"> ٦٠ عمرو بن قميئة:</span> ٢: ١٩٨ ح عمرو بن ناعصة: ٢: ١٧٧ ابن عون: ٢: ٢٠٨ عيسى بن عمر: ١: ٥٠،<span class="c5"> ٢:</span> ٢١، ١٧٠،<span class="c5"> ٣١٥ عيسى بن مريم:</span> ١: ٩٤، ١٦٦، ١٦٨،<span class="c5"> ١٨٣ أبو عيينة حصن بن بدر:</span> ١: ٣٥٨<a href="#p1" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
