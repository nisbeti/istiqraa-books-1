<div class="nass margin-top-10"> <p><span id="p1" class="anchor"></span><span class="c5">رجف:</span> ترجف الراجفة ٢/ ٢٨٤ رجل: رجالا ١/ ٧٦، ٢/ ٤٩،<span class="c5"> رجلك ١/ ٣٨٤ جم:</span> رجيم ١/ ٣٤٨، رجما ١/ ٣٩٨،<span class="c5"> ظن مرجم ١/ ٣٩٨ رجه:</span> أرجه ١/ ٢٢٥،<span class="c5"> ٢/ ٨٥ رجو:</span> مرجون ١/ ٢٦٩، يرجون ١/ ٢٧٥، ٢/ ٧٣، ٢١٠، أرجو ٢/ ١١٥ ترجى ٢/ ١٣٩، أرجاء ٢/ ٢٦٨،<span class="c5"> ترجون ٢/ ٢٧١ رحب:</span> رحبت ١/ ٢٧١،<span class="c5"> مرحبا ٢/ ١٨٦ رحق:</span> رحيق ٢/ ٢٨٩ رحل: المرحل ١/ ١٣٧ رحم: الرحمان ١/ ٢١، الرحيم ١/ ٢١، رحموت ١/ ١٩٨،<span class="c5"> رحما ١/ ٤١٢ رخو:</span> رخاء ٢/ ١٨٣ ردء: ردءا ٢/ ١٠٤ ردد: فردوه ١/ ١٣٠، نرد ١/ ١٩٦، فردوا أيديهم ١/ ٣٣٦، رددنا ١/ ٣٧١،<span class="c5"> فارتدا ١/ ٤٠٩ ردف:</span> مردفين ١/ ٢٤١، ردف ٢/ ٩٦،<span class="c5"> الرادفة ٢/ ٢٨٤ ردى:</span> المتردية ١/ ١٥١، فتردى ٢/ ١٧،<span class="c5"> لتردين ١/ ١٧٠ رذل:</span> أرذل ٢/ ٤٥ رسخ: الراسخون ١/ ٨٦ رسس: الرس ٢/ ٧٥،<span class="c5"> ٢٢٣ رسل:</span> رسول ١/ ١٣، ٢/ ٨٤، الرسل ١/ ٣٤٣،<span class="c5"> المرسلات ٢/ ٢٨٠ رسو:</span> مرساها ١/ ٢٨٩، ٢/ ٢٨٥، رواسى ١/ ٣٤٨، ٣٥٧،<span class="c5"> ٢/ ١٢٦ رصد:</span> مرصد ١/ ٢٥٣،<span class="c5"> مرصوص ٢/ ٢٥٧ رضى:</span> رضيت ١/ ١٥٣،<span class="c5"> راضية ٢/ ٢٦٨ رعد:</span> الرعد ١/ ٣٢٥ رعى: راعنا ١/ ٤٩،<span class="c5"> رعيا ١/ ٢٩٨ رغد:</span> ١/ ٣٦٩ رغم: مراغما ١/ ١٣٨ رفث: الرفث ١/ ٦٧،<span class="c5"> رفث ١/ ٧٠ رفد:</span> الرفد المرفود ١/ ٢٩٨<a href="#p1" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>