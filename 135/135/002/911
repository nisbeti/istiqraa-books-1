<div class="nass margin-top-10"> <p><span id="p1" class="anchor"></span><span class="c5">وسق:</span> ٢/ ٢٩١،<span class="c5"> اتسق ٢/ ١٩١ وسل:</span> الوسيلة ١/ ١٦٤ وسم: المتوسمين ١/ ٣٥٤ وسن: سنة ١/ ٧٨ وصب: واصبا ١/ ٢٦١،<span class="c5"> واصب ٢/ ١٦٦ وصد:</span> بالوصيد ١/ ٣٩٧ وصل: وصيلة ١/ ١٧٨،<span class="c5"> وصلنا ٢/ ١٠٨ وصى:</span> تواصوا ٢/ ٢٢٧ وضع: لا وضعوا ١/ ٢٦١ وضن: موضونة ٢/ ٢٤٨،<span class="c5"> الوضين ٢/ ٢٤٨ وطأ:</span> يواطئوا ١/ ٢٥٩،<span class="c5"> وطئا ٢/ ٢٧٣ وطر:</span> وطرا ٢/ ١٣٨ وعد: وعد ١/ ٢٧٣، ٢/ ١٨٩،<span class="c5"> الوعد ٢/ ١٤٩ وعظ:</span> موعظة ١/ ٨٣،<span class="c5"> يوعظون ١/ ١٣١ وعل:</span> الوعل ١/ ١٤٠ وعى: واعية ٢/ ٢٦٧،<span class="c5"> يوعون ٢/ ٢٩٢ وفد:</span> وفدا ٢/ ١١ وفق: وفاقا ٢/ ٢٨٢ وفى: الموفون ١/ ٦٥، وفيت وأوفيت ١/ ١٤٥،<span class="c5"> يتوفاكم ٢/ ١٣١ وقت:</span> موقوتا ١/ ١٣٩ وقد: وقودها ١/ ٣٤،<span class="c5"> أوقدوا ١/ ١٧١ وقذ:</span> الموقوذة ١/ ١٥١ وقر: وقرا ١/ ١٨٩،<span class="c5"> ٣٨٠ وقع:</span> الوقيع ١/ ٣٤٣،<span class="c5"> وقعت الواقعة ٢/ ٢٤٧ وقل:</span> وقل ٢/ ٧٢ وقى: تقاة ١/ ٩٠ وكأ: متكئا ١/ ٣٠٩ وكز: فوكزه ٢/ ٩٩<a href="#p1" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>