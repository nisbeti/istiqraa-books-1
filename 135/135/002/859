<p><span class="anchor" id="p2"></span><span class="c4">[أ]</span><a href="#p2" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p3"></span><span class="c5">أن أأنت ١/ ١٨٣ أبب:</span> أبأ ٢/ ٢٨٦ أبق: أبق ٢/ ١٧٤ أبلى: أبابيل ٢/ ٣١٢ أبس: إبليس ١/ ٣٨ أبو: آباؤك ١: ٥٧، آباؤنا ١/ ٦٣،<span class="c5"> ٢/ ١٦٨ أتو:</span> إتاوة ٢/ ٦١ أتى: وآتوا ١/ ٥١، ٢٥٣، إيتاء ١/ ٣٦٧، حيث أتى ٢/ ٢٣، أوتى ٢/ ١٠٧، لأتوها ٢/ ١٣٤، نؤتها ٢/ ١٣٧، أتيت ٢/ ١٣٨،<span class="c5"> آتينا ٢/ ١٤٢ أثث:</span> أثاثا ١/ ٣٦٥،<span class="c5"> ٢/ ١٠ أثر:</span> فأثرن ٢/ ٣٠٧،<span class="c5"> أثارة ٢/ ٢١٢ أثم:</span> أثاما ٢/ ٨١، إثما ١/ ١٠٩، ١٣٩، أثيم ٢/ ٩١، تأثيم ٢/ ٢٤٩،<span class="c5"> آثما ٢/ ٢٨٠ أجج:</span> أجاج ٢/ ٧٧، ١٥٣،<span class="c5"> ٢٥٢ أجر:</span> أجز ١/ ١٣٨، ٢٢٥، ٤١١، ٢/ ٨٥، أجورهن ١/ ١٥٤، ٢/ ١٣٨،<span class="c5"> تأجرنى ٢/ ١٠٢ أجل:</span> أجلهن ١/ ٧٥،<span class="c5"> ٢/ ١٠٢ أحد:</span> قل هو الله أحد ١/ ٦، ٢/ ١٣٧،<span class="c5"> ٣١٦ أخذ:</span> أخذ الله ١/ ١٩٢، أخذنا ١/ ٢٤٤، أخذت ٢/ ١٥٤، اتخذوني ١/ ١٨٤،<span class="c5"> يؤاخذ ٢/ ١٥٦ أخر:</span> أخراكم ١/ ١٠٥ أخو: أخو ١/ ٩، ١١٨، إخوانكم ١/ ٢٥٣،<span class="c5"> ٢/ ١٣٤ أدد:</span> إدا ٢/ ١١<a href="#p3" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>