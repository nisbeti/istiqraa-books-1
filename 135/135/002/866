<div class="nass margin-top-10"> <p><span id="p1" class="anchor"></span><span class="c5">ثمر:</span> ثمره ١/ ٢٠٧،<span class="c5"> ثمر ١/ ٤٠٢ ثمن:</span> الثمن ١/ ١١٨ ثنى: المثاني ١/ ٦، مثانى ١/ ٧، مثنى ١/ ١١٤، ٢/ ١٥٠،<span class="c5"> ١٥٢ ثوب:</span> استثاب ١/ ٤٩، مثوبة ١/ ٤٩، ١٧١، مثابة ١/ ٥٤،<span class="c5"> ثوب ٢/ ٢٩٠ ثور:</span> أثاروا ٢/ ١١٩ ثوى: مثواه، أم مثوى، أبو مثوى ١/ ٣٠٤<a href="#p1" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p2"></span>[<span class="c2">(ج)</span>]<a href="#p2" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p3"></span><span class="c5">جأر:</span> تجأرون ١/ ٣٦١،<span class="c5"> يجأرون ٢/ ٦٠ جبب:</span> الجب ١/ ٣٠٢ جبت: الجبت ١/ ١٢٩ جبل: الجبلة ٢/ ٩٠،<span class="c5"> جبلا ٢/ ١٦٤ جبى:</span> يجبى ١/ ١٠٩، اجتبيناهم ١/ ٢٠٠، يجتبيك ١/ ٣٠٢، اجتباه ١/ ٣٦٩ اجتبينا ٢/ ٨،<span class="c5"> يجبى ٢/ ١٠٨ جثث:</span> اجتثت ١/ ٣٤٠ جثم: جاثمين ١/ ٢١٨،<span class="c5"> ٢/ ١١٦ جثى:</span> جثيا ٢/ ٩،<span class="c5"> جاثية ٢/ ٢١٠ جدث:</span> الأجداث ٢/ ١٦٣ جدف: جدف ٢/ ١٦٣ جدل: جدال ١/ ٧٠ جذذ: جذاذ ٢/ ٤٠،<span class="c5"> مجذوذ ١/ ٢٩٩ جذع:</span> جذوع ٢/ ١٩٥،<span class="c5"> فى جذوع ٢/ ٢٣٣ جذوة:</span> جذوة ٢/ ١٠٢ جرح: جرحتم ١/ ١٥٤، ١٩٤، الجوارح ١/ ١٥٤،<span class="c5"> اجترحوا ٢/ ٢١٠ جرز:</span> جرز ١/ ٣٩٣،<span class="c5"> الجرز ٢/ ١٣٣ جرف:</span> جرف ١/ ٢٦٩ جرم: يجرمنكم ١/ ١٤٧، إجرام ١/ ٢٨٨.<span class="c5"> لا جرم ١/ ٣٥٨ جرى:</span> جرين بهم ١/ ١١، مجراها ١/ ٢٨٩، يجرى ١/ ٣٢١<a href="#p3" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>