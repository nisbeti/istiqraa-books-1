<div class="nass margin-top-10"> <p><span id="p1" class="anchor"></span><span class="c5">برر:</span> البر ١/ ١٣، ٦٥، ٦٨،<span class="c5"> ١٤٠ برز:</span> بارزة ١/ ٤٠٦، برزخ ٢/ ٦٢، ٧٧،<span class="c5"> ٢٤٣ برم:</span> أبرموا ٢/ ٢٠٦ بره: برهان ١/ ٥١، ١٤٤،<span class="c5"> ٢/ ٦٢ بزغ:</span> بازغا ١/ ٢٠٠ بسب: البسابس ١/ ١٨٩ بسر: بسر ٢/ ٢٧٥ بسس: بست ٢/ ٢٤٧ بسط: بسطة ١/ ٧٧، بسط ١/ ١٦١، كباسط ١/ ٢٧، البسط ١/ ٣٧٥،<span class="c5"> يبسط ٢/ ١٤٩ بسق:</span> باسقات ٢/ ٢٢٣ بسل: تبسل ١/ ١٩٤،<span class="c5"> أبسلوا ١/ ١٩٥ بشر:</span> يبشرك ١/ ٩١، تبشرون ١/ ٣٥٢،<span class="c5"> يبشر ٢/ ٢٠٠ بصر:</span> أبصاركم ١/ ١٩٢، مبصرا ١/ ٢٧٩، ٢/ ٩٦، البصير ١/ ٢٨٧، بصير ١/ ٣١٨ بصائر ١/ ٢٠٣، ٢٣٧، ٢/ ٢١٠، بصيرة ١/ ٣١٩، ٢/ ٢٧٧، بصرت ٢/ ٢٦،<span class="c5"> تبصرة ٢/ ٢٢٣ بضع:</span> بضع ٢/ ١١٩ بطر: بطرت ٢/ ١٠٨ بطش: يبطش ٢/ ١٠٠،<span class="c5"> نبطش ٢/ ٢٠٨ بطن:</span> بطانة ١/ ١٠٣،<span class="c5"> بطنه ٢/ ٦٨ بعث:</span> يبعثون ١/ ٣٥٧، بعثناهم ١/ ٣٩٧، بعثرت ٢/ ٢٨٨،<span class="c5"> بعثر ٢/ ٣٠٨ بعد:</span> بعد ذلك ١/ ١٤، ٢/ ٢٦٥، بعدا ١/ ٢٩٨، بعيد ٢/ ٩٣،<span class="c5"> باعد ٢/ ١٤٧ بعض:</span> ما بعوضة ١/ ٣٤، ببعضها ١/ ٤٥،<span class="c5"> بعض ٢/ ٢٠٥ بعل:</span> بعولتهن ١/ ٧٤، ٢/ ٦٥،<span class="c5"> بعلا ٢/ ١٧٢ بعو:</span> بعونا ١/ ١٩٥ بغت: بغتة ١/ ١٩١،<span class="c5"> ٣١٩ بغى:</span> باغ ١/ ٦٤، تبغوا ١/ ١٢٥، تبتغى ١/ ١٨٠، أبغيكم ١/ ٢٢٧، يبغونها ١/ ٣٣٥، البغاء ٢/ ٦٦، ينبغى ٢/ ٧١،<span class="c5"> بغيا ٢/ ١٩٩ بقر:</span> البقرة ١/ ٦، ٨٦، باقية ٢/ ٢٦٧<a href="#p1" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>