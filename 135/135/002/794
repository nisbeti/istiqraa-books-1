<p><span class="anchor" id="p2"></span><span class="c5">عامر بن جوين:</span> إبقالها ٢: ٦٧/ ٦٠٣ عامر الخصفى: لزور ١: ٦٦،<span class="c5"> ٦٧/ ٨٢ عامر بن الطفيل:</span> ١: ٢٥ ح-<span class="c5"> بالمرصد ١:</span> ٢٥٣/ ٢٨٣-<span class="c5"> محضر ١:</span> ٩٢،<span class="c5"> ٢:</span> ١/ ١١٤ العباس بن مرداس: ١: ٧٩، ١٣١،<span class="c5"> ٢:</span> ٤٥، ١٩٥/ ١٠٠-<span class="c5"> حاذرا ١:</span> ٢٤٢/ ٢٧٥-<span class="c5"> لا يراها ٢:</span> ٨١، ١٠٢، ٢٣٥/ ٦٢٨-<span class="c5"> منيع ٢:</span> ٨٦/ ٦٤٢- جداعة، قناعة، سراعه،<span class="c5"> ٢:</span> ١٧٧/ ٧٨٨-<span class="c5"> منتهاها ٢:</span> ٨٤/ ٦٣٧ عبد الرحمن بن حسان بن ثابت: ١: ٢٠٣،<span class="c5"> ٢:</span> ١٧٠ عبد الله بن رواحة: ١: ٢١/ ٩ عبد الله بن الزبعرى: بور ١: ٣٤٠،<span class="c5"> ٢:</span> ٧٣، ٢١٧، ٢٦٣/ ٣٩٠-<span class="c5"> ورمحا ٢:</span><a href="#p2" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p3"></span><span class="c5">٦٨/ ٦٠٦ عبد الله بن عنمة:</span> وموحدا ١: ١١٤/ ١٣٥ عبد الله بن قيس الرقيات: أن غضبوا ١: ١٧٠/ ٢٠٣- غسقا،<span class="c5"> والأرقا ١:</span> ٣٨٨-<span class="c5"> الملك ٢:</span> ٦١/ ٦٣٠-<span class="c5"> قدما ٢:</span> ٥٠/ ٦٠٦ عبد مناف بن ربع الهذلي: الشردا ١: ٣٧، ٥٧،<span class="c5"> ٢:</span> ١٩٢/ ٤٦- العضدا، البردا،<span class="c5"> الشردا ١:</span> ٣٣١/ ٤٦-<span class="c5"> لبدا ٢:</span> ٢٧٢/ ٩١٧ عبد يغوث بن وقاص: وعاديا ١: ٢٥٧/ ٢٩٠ عبيد بن الأبرص: الأريب ١: ٣٠/ ٣١-<span class="c5"> المذءوب ٢:</span> ٤٢/ ٥٨٨-<span class="c5"> يؤب ٢:</span><a href="#p3" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p4"></span><span class="c5">١٨٠/ ٧٩٢ عبيدة بن همام:</span> نكر،<span class="c5"> لحر ١:</span> ١٣٣/ ١٥٨ العجاج: ٢: ٣١-<span class="c5"> أمت ٢:</span> ٣٠/ ٥٦٤-<span class="c5"> تعمجا ٢:</span> ٢٢٠/ ٨٥٣-<span class="c5"> تعوجا ١:</span> ٤١٣/ ٤٩٠- الخروجا،<span class="c5"> رجوجا ٢:</span> ٢٢٣/ ٨٦١-<span class="c5"> ممرجا ٢:</span><a href="#p4" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p5"></span>٧٧/ ٦٢٠-<span class="c5"> يهرجا ١:</span> ٢٩٢/ ٢٢٧- الطبخ، مستصرخ،<span class="c5"> مفنخ ٢:</span><a href="#p5" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p6"></span>١٠٠/ ٦٦٩- الصداد،<span class="c5"> الممتاد ١:</span> ٣٠١، ٣٤١/ ٣٤١-<span class="c5"> آدا ١:</span> ٤٦،<span class="c5"> ٢:</span> ١٧٩/ ٥١- سطر، قدر،<span class="c5"> النتر ١:</span> ٣٨٣،<span class="c5"> ٢:</span><a href="#p6" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
