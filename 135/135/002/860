<div class="nass margin-top-10"> <p><span id="p1" class="anchor"></span><span class="c5">إذ:</span> وإذ ١/ ٣٦، ٢٣١، وإذ قال ١/ ١١، ٣٥، ٤١، ١٨٣، إذ قالت ١/ ٩٠، وإذ قالت ١/ ٩٣،<span class="c5"> وإذ تأذن ١/ ٣٣٥ أذن:</span> فأذنوا ١/ ٨٣، أذان ١/ ٢٥٢، آذنتكم ٢/ ٤٣،<span class="c5"> أذنت ٢/ ٢٩١ أذى:</span> أوذى ٢/ ١١٤ أرب: مأربة ٢/ ١٧، مآرب ٢/ ١٧،<span class="c5"> الإربة ٢/ ٦٥ أرك:</span> الأرائك ١/ ٤٠١،<span class="c5"> ٢/ ١٦٤ أزر:</span> أزرى ٢/ ١٨،<span class="c5"> فآزره ٢/ ٢١٨ أزز:</span> تؤزهم أزا ٢/ ١١ أزف: أزفت الآزفة ٢/ ٢٣٩ أست: الإستبرق ١/ ١٧،<span class="c5"> ٢/ ٢٤٥ أسر:</span> أسرهم ٢/ ٢٨٠ أسس: الأس ١/ ٥٥ أسف: أسف ١/ ٢٢٨، ٣٩٣، يا أسفى ١/ ٣١٦،<span class="c5"> آسفونا ٢/ ٢٠٥ اسم:</span> باسم الله ١/ ١١،<span class="c5"> ٢١ أسن:</span> آسن ٢/ ٢١٥ أسى: فلا تأس ١/ ١٦١، ١٧١،<span class="c5"> أسى ١/ ٢٢٢ أشر:</span> الأشر ٢/ ٢٤١ أصب: أصب ١/ ٣١١ أصد: مؤصدة ٢/ ٢٩٩،<span class="c5"> ٣١١ أصر:</span> إصر ١/ ٨٤،<span class="c5"> ٩٧ أصل:</span> الآصال ١/ ٢٣٩، أصيلال ١/ ٣٢٨،<span class="c5"> أصيلا ٢/ ١٣٨ أفف:</span> أف ١/ ٣٧٤ أفك: يؤفكون ١/ ١٧٤، ٢٥٧، إفك ٢/ ٦٣، ٧٠، لتأفكنا ٣/ ٢١٣، المؤتفكات ١/ ٢٦٣،<span class="c5"> المؤتفكة ٢/ ٢٣٩ أفل:</span> أفل ١/ ١٩٩،<span class="c5"> آفلون ١/ ٢٠٠ أكل:</span> فليأكل ١/ ١١٧، أكال ١/ ١٨٦، الأكل ١/ ٣٢٢، أكلها ١/ ٣٤٠<a href="#p1" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>