<p><span class="anchor" id="p6"></span><span class="c5">جابر بن عبد الله:</span> ١: ٨٩ أبو جعفر النحاس: ٢: ١٧٣ أبو جعفر القارئ: ١: ١٤ ح خنادة بن عوف: ١: ٢٥٩ ح<a href="#p6" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p7"></span>[<span class="c2">(ح)</span>]<a href="#p7" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p8"></span><span class="c5">أبو حاتم السجستاني:</span> ١: ٦ ح، ٢١٣ ح،<span class="c5"> ٢:</span> ١٢ ح، ١٥ ح، ٣٩ ح، ١٥٩ ح،<span class="c5"> ١٩٥ ح الحارث بن الشريد:</span> ١: ٢٨ الحارث بن شمس الغساني: ٢: ٢٢٩ ح الحارث بن عمرو بن قيس: ١: ٢٥٩ ح الحجاج: ٢: ١٨٣ الحسن البصري: ١: ١٤٩ ح،<span class="c5"> ٢:</span> ٢١٢ ح الحصين بن الحمام: ١: ٢١ ح حفص: ١: ١٥٥ ح،<span class="c5"> ٢:</span> ٢ ح حماد بن زيد: ١: ٥٧ حمزة القاري: ١: ١٤ ح، ٨٠ ح، ٩١ ح، ١١٣ ح، ١٢٩ ح، ١٤١ ح، ١٧٤ ح، ١٨١ ح،<span class="c5"> ٢١٧ ح حمصيصة الشيباني:</span> ٢: ٦<a href="#p8" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
