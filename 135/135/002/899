<p><span class="anchor" id="p2"></span><span class="c5">كأس:</span> كأس ٢/ ١٦٩ كأن: فكأين ٢/ ٥٢ وكأين ٢/ ١١٦ كتب: يكبتهم ١/ ١٠٣،<span class="c5"> كبتوا ٢/ ٢٥٥ كبد:</span> كبد ٢،<span class="c5"> ٢٩٩ كبر:</span> يكبر ١/ ٣٨٢ لكبيركم ٢/ ٢٣، كبره ٢/ ٦٤، أكبرنه ١/ ٣٠٩، الكبرى ٢/ ١٨،<span class="c5"> كبارا ٢/ ٢٧١ كبك:</span> كبكبوا ٢/ ٨٧ كبى: كباء ١/ ٢٧٤ كتب: كتب ١/ ٦٦، ١٦٠، ٢٦٢، ٢/ ٢٥٥، كتبت ١/ ١٣٢، كتبنا ١/ ١٣١، كتبناها ٢/ ٢٥٤، سيكتب ١/ ١١٠، يكتبون ٢/ ٢٣٤، أوتوا الكتاب ١/ ٩٠، كتاب ١/ ٢١٠، ٢٨٥، ٣٣٥، ٣٤٦،<span class="c5"> الكتاب/ ٢٧٢ كثر:</span> تستكثر ٢/ ٢٧٥ كدح: كادح ٢/ ٢٩١ كدر: انكدرت ٢/ ٢٨٨ كذب: كذاب ٢/ ٢٨٣ كدى: أكدى،<span class="c5"> كدية ٢/ ٢٣٨ كرب:</span> كرب ٢/ ١٨٣ كرر: الكرة ١/ ٣٧١ كرم: كرمنا ١/ ٣٨٦،<span class="c5"> أكرمكم ٢/ ٢٢١ كسف:</span> كسفا ٢/ ٩١،<span class="c5"> ٢٣٤ كسل:</span> كسالى ١/ ٢٦٢ كشف: يكشف ٢/ ٢٦٦ كظم: كظيم ١/ ٣٦١،<span class="c5"> مكظوم ٢/ ٢٦٦ كعب:</span> الكعبان ١/ ١٥٥ كفت: كفاتا ٢/ ٢٨١<a href="#p2" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>