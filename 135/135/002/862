<p><span class="anchor" id="p3"></span><span class="c5">بأساء:</span> البأساء ١/ ١٩١، بئيس ١/ ٢٣١،<span class="c5"> لبئس ٢/ ٤٦ بال:</span> بال ٢/ ٢٠،<span class="c5"> ٢١٤ بتر:</span> الأبتر ٢/ ٣١٤ بتك: فليبتكن ١/ ١٤٠ بثث: بث ١/ ٦٢، ٣١٧،<span class="c5"> مبثوثة ٢/ ٢٩٦ بجس:</span> فانبجست ١/ ٢٣٠ بحر: البحيرة ١/ ١٧٩، ١٨٠،<span class="c5"> البحر ٢/ ١٢٨ بخس:</span> لا يبخس ١/ ٨٣، ولا تبخسوا ١/ ٢١٩، بخس ١/ ٣٠٤،<span class="c5"> تبخسوا ٢/ ٩٠ بخع:</span> باخع ١/ ٣٩٣،<span class="c5"> ٢/ ٨٢ بدء:</span> بادىء ١/ ٢٨٧،<span class="c5"> يبدىء ٢/ ١١٥ بدر:</span> بدارا ١/ ١١٧،<span class="c5"> البدر ١/ ٣١٩ بدع:</span> بديع ١/ ٥٢،<span class="c5"> ٢٠٣ بدل:</span> تبديلا ٢/ ١٥٦ بذر: المبذر ١/ ٣٧٤ برء: براءة ١/ ٢٥٢، براء ٢/ ٢٠٣،<span class="c5"> نبرأها ٢/ ٢٥٤ برج:</span> بروج ١/ ١٣٢، ٣٤٨، يتبرجن ٢/ ١٣٨، البروج ٢/ ٢٩٣،<span class="c5"> متيرجات ٢/ ٦٩ برد:</span> بارد ٢/ ٢٥١، برد ٢/ ٢٨٢<a href="#p3" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>