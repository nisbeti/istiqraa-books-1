<p><span class="anchor" id="p2"></span>[<span class="c2">(ا)</span>]<a href="#p2" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p3"></span><span class="c5">إبراهيم النبي:</span> ١: ٥٧، ٥٨، ٥٩، ١٠٠ ح، ٢٠٨، ٢٩٣،<span class="c5"> ٢:</span> ٤٩،<span class="c5"> ٢٠٣ الأثرم على بن المغيرة:</span> ١: ١، ١١٦ ح، ١٥٧، ١٧٩، ٣٠٤، ٣٧٣،<span class="c5"> ٢:</span> ١٧٣ ح آدم النبي: ١: ٣٨ أبو الخطاب الأخفش: ١: ١٨٩ ح،<span class="c5"> ٢:</span> ٢١،<span class="c5"> ٢٦٩ إسحاق النبي:</span> ١: ٥٧،<span class="c5"> ٢:</span> ٤٠ ابن إسحاق: ٢: ١٩٣ أبو إسحاق: ١: ٣٧٦ أبو أسلم: ١: ٤٧ أسماء بنت عميس: ١: ١٢٢،<span class="c5"> ٢:</span> ٣٣ ح إسماعيل: ١: ٥٧ إسماعيل بن صبيح الكاتب: ١: ١ ح أبو الأسود الدؤلي: ٢: ٢١٦ الأشعث بن قيس: ١: ١٢١ الأصمعى: ١: ١ ح، ١٦٣،<span class="c5"> ٢:</span> ٢٢،<span class="c5"> ٧٣ الأعمش:</span> ١: ١٤ ح أبو أمامة التابعي: ١: ٢٦٣ أنس بن زياد العبسي: ٢: ٨ ح أيوب بن أبى تيمية: ١: ٥٧<a href="#p3" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p4"></span>[<span class="c2">(ب)</span>]<a href="#p4" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p5"></span><span class="c5">البراص بن قيس:</span> ١: ٢٤٨ بشر بن هلال: ٢: ٢١ أبو بكر الصديق: ١: ١٨١ ح،<span class="c5"> ٢:</span> ٢ ح ٩٨، ٢٧٩<a href="#p5" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>