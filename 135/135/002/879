<p><span class="anchor" id="p3"></span><span class="c5">سئل:</span> وسل ١/ ٨، ٤٧، وسئل القرية ١/ ٢٩٧،<span class="c5"> مسئولا ١/ ٣٧٩ فسئلوهم ٢/ ٤٠ سبب:</span> الأسباب ١/ ٦٣، سبب ٢/ ٤٧،<span class="c5"> السبب ٢/ ١٧٨ سبت:</span> سبات ٢/ ٢٨٢ سبح: نسبح ١/ ٣٦، سبحانك ١/ ٣٦، يسبحون ٢/ ٣٨، يسبحون ٢/ ١٩٢، سبحا ٢/ ٢٧٣،<span class="c5"> السابحات سبحا ٢/ ٢٨٤ سبط:</span> أسباط ١/ ٢٣٠ سبع: السبع الطول ١/ ٦، السبع ١/ ١٥١،<span class="c5"> سبعا من المثاني ١/ ٣٥٤ سبق:</span> سابقين ٢/ ١١٦،<span class="c5"> سبقوا ١/ ٢٤٩ سبل:</span> سبيل ١/ ٣١٩، ٣٥٤، ٢/ ١٢٧، السبيل ١/ ٣٥٧، سبيلا ٢/ ٧٤، ٧٨،<span class="c5"> السبيلا ٢/ ١٤١ سبى:</span> الإسباءة ١/ ٢٣٨ سجد: ساجدين ١/ ١٠، السجود ١/ ٥٤، ألا تسجد ١/ ٢٦، ٢١١، يسجدوا ٢/ ٩٣،<span class="c5"> يسجدان ٢/ ٢٤٢ سجر:</span> مسجورة ٢/ ٥،<span class="c5"> المسجور ٢/ ٢٣٠ سجل:</span> سجيل ١/ ١٨، ٢٩٦، ٢/ ٣١٢<a href="#p3" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>