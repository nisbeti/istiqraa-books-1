<div class="nass margin-top-10"> <p><span id="p1" class="anchor"></span><span class="c5">خفى:</span> خفية ١/ ١٩٤، أخفى ٢/ ١٦،<span class="c5"> خفيت ٢/ ١٧ خلد:</span> أخلد ١/ ٢٣٣،<span class="c5"> مخلدون ٢/ ٢٤٩ خلص:</span> بخالصة ٢/ ١٨٥،<span class="c5"> المخلصين ٢/ ١٨٧ خلط:</span> الخلاط ٢/ ٣٧،<span class="c5"> الخلطاء ٢/ ١٨١ خلع:</span> خلعة ١/ ٨١ خلف: خلاف ١/ ١٦٤، ٢٦٤، خلائف ١/ ٢٠٩، فخلف ١/ ٢٣٢، الخالفين ١/ ٢٦٥ الخوالف ١/ ٢٦٥، خليف ١/ ٢٧٣، خلفك ١/ ٢٨١،<span class="c5"> يخالفون ٢/ ٦٩ خلفة ٢/ ٧٩ خلق:</span> خلاق ١/ ٤٨، ٩٧، ٢٦٣، مخلقة ٢/ ٤٤، الخلفاء ٢/ ٧٢،<span class="c5"> خلق ١/ ١١١ تخلقون ٢/ ١١٤ خلل:</span> خلة ١/ ٧٨، خلال ١/ ٣٤١، ٣٧٠، خلالهما ١/ ٤٠٢، خلاله ٢/ ٦٨،<span class="c5"> ١٢٤ خلو:</span> خلوا ١/ ٧٢، خلت ١/ ١٠٣،<span class="c5"> ٣٣٠ خمص:</span> مخمصة ١/ ١٥٣، ٢٧١، خامدين ٢/ ٣٦،<span class="c5"> خمط ٢/ ١٤٧ خنت:</span> الخنوت ١/ ١٦٣ خنس: الخنس ٢/ ٢٨٧،<span class="c5"> الخناس ٢/ ٣١٧ خنق:</span> المنخنقة ١/ ١٥١ خور: خوار ١/ ٢٨٨ خوض: يخوضوا ١/ ١٤١،<span class="c5"> خوض ٢/ ٢٣١ خوف:</span> أن يخافا ١/ ٧٤، خفتم ١/ ٧٤، ١١٤، ١١٦، ١٢٦، المخافة ١/ ١٤٠، خيفة ١/ ٢٣٨ ٢٩٣، ٢/ ٢٣، واما تخافن ١/ ٢٤٩،<span class="c5"> خوفا ١/ ٣٢٥ خول:</span> خوله ٢/ ١٨٨ خون: خائنة ١/ ١٥٨ خوى: خاوية ١/ ٨٠،<span class="c5"> ٤٠٥ خيب:</span> خاب ١/ ٢٦٩،<span class="c5"> خائب ١/ ٢٦٩ خير:</span> خير ١/ ٥٠، ١٠٩، ١٤٣، اختار ١/ ٢٢٩، الخيرات ١/ ٢٦٧، الأخيار ٢/ ١٨٥، خيرات ٢/ ٢٤٦،<span class="c5"> هو خيرا ٢/ ٢٧٤ خيط:</span> الخيط ١/ ٦٨ خيل: مختال ١/ ١٢٧<a href="#p1" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>