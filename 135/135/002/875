<p><span class="anchor" id="p3"></span><span class="c5">رءف:</span> رءوف ١/ ٥٩،<span class="c5"> ٢٧٠ رأى:</span> رأيت ١/ ١٢، أرنا ١/ ٥٥، يرى ١/ ٦٢، يرونهم ١/ ٨٨، ألم تر ١/ ١٢٩، ٣٣٩، رئيا ٢/ ١٠، أفرأيت ٢/ ١٠،<span class="c5"> لنريك ٢/ ١٨ ربب:</span> ربانيين ١/ ٩٧، ربيون ١/ ١٠٤،<span class="c5"> ربائب ١/ ١٢١ ربث:</span> يربث ١/ ١٥٣ ربص: يتربصن ١/ ٧٤ ربط: رابطوا ١/ ١١٢،<span class="c5"> ربطنا ١/ ٣٩٤ ربع:</span> الربع ١/ ٢٠٩،<span class="c5"> رباع ٢/ ١٥٢ ربو:</span> ربوة ١/ ٨٢، ٢/ ٥٩، رابيا ١/ ٣٢٨، أربى ١/ ٣٦٧، يربو ٢/ ١٢٣،<span class="c5"> رابية ٢/ ٢٦٧ رتع:</span> نرتع ١/ ٣٠٣ رتق: رتقا ١/ ١٣،<span class="c5"> ٢/ ٣٦ رجج:</span> رجت رجا ٢/ ٢٤٧ رجز: الرجز ١/ ٤١، ٢٢٧،<span class="c5"> رجز ١/ ٢٤٢ رجس:</span> الرجس ١/ ٢٠٦،<span class="c5"> رجس ١/ ٢١٨ رجع:</span> رجع ٢/ ٢٢٢، الرجع ٢/ ٢٩٤، الرجعى ٢/ ٣٠٤<a href="#p3" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>