<div class="nass margin-top-10"> <p><span id="p1" class="anchor"></span>الأقيشر الغضب ٢٨٢<a href="#p1" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p2"></span>امرؤ القيس أحسبا ٣٦، المعذّب ٢٤، ٢٠٠، مهذب ٢٤، ٢٥، ١١٢، القباب ٣٦، تطيب ١٩٩، ٢٧١، لأثرا ٧٢، ٣١١، ستره ٢٣، دبر ٣٢، منتشر ٣٢، ١١٥، منهمر ٣٣، حجر ٤١، سكر ٦٥، والخصر ١٣٠، أنفسا ١٠٧ الخالى ٩، المال ٢٢، ٢٣ ليبتلى ٢٧، بكلكل ٢٧، ٣٠، ٣٤، بأمثل ٢٨، ٢٩، بيذبل ٢٨، جندل ٢٨، الرحل ٣٠، خلخال ٣١، إجفال ٣١، يفعل ٣٢ بأعزل ٣٢، ١٢٩، محول ٣٤، ١٥٠، المفصل ٣٤، واغل ١٢٦، نابل ١٤٠، المتفضل ١٥٠، حال ١٥٠، مقتّل ١٩٥، الحسان ٤٦٠، العصىّ ٢٢، ٢٣، نعىّ ١١٧<a href="#p2" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p3"></span>أمية بن أبى الصلت بتأيّد ١٠٥، ذائقها ٩٥، إسرال ٢٩٨، والأكبال ٢٩٨<a href="#p3" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p4"></span>أبو الأهتم المؤمنينا ٤٤٨<a href="#p4" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p5"></span>أوس بن حجر وخنزير ١١١، جدعا ٧٤، ومخولا ١١٨<a href="#p5" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p6"></span>أوس بن مغراء وعيدها ٧٨<a href="#p6" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p7"></span>أيمن بن خريم يزيدا ٢٨٤، الأقعس ٢٨٣<a href="#p7" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p8"></span>أبو أيوب بن أخت أبى الوزير البواتر ٤٣٥<a href="#p8" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p9"></span>الباهلى رطب ٣٧٠<a href="#p9" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p10"></span><span class="c2">(ب)</span><a href="#p10" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p11"></span>البحترى الحسناء ٤١٤، عطاؤك ٣٧٣، وعجائبه ٤١٦، كاتبه ٤١٦، غربا ٤٢١، الذاهب ٣٧٥، توهب ٤٢٠، للمغرب ٤٢١، كذب ٤١٥، مواعدا ٤٢٥، قاعد ٤١٤، العماد ٤١٥، بحاسد ٤٢١، وشرده ٤٢٣، رعوده ٤٢٥، المردد ٤١٣، الكبير ٤١١، الغزار ٤١٧، حبس ٤٣٠، بالمقراض ٤١٤، متالع ٤١٣، يشرق ٤٢١، أليقا ٤١٥، المخلوق ٤٢٤، فاصدق ٤٢٦، لا يسأل ٤١٢ ومالى ٤٢٣، أشباله ٤١٨، أمواله ٤١٩، ابتذاله ٤٢٢، الأقدام ٤١٣، مظلما ٣١، تكرّما ٦٦، النعما ٤١٢، وأعجم ٤٢٢، يهرم ٤٢٤، دوامه ٤٢٢<a href="#p11" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p12"></span>بشار بن برد لا تخاطبه ١٢١، ٣١٧، الزيت ٣١٧، منهج ٣١٥، بعدى ٤٥٠، تنبذ ٣١٤، مشير ٣١٣، قصار ٣١٧، ٣٦٦، الدنانير ٣١٨، زهر ٣١٤ تجرى ٣١٤، البصل ٣١٥، الجمل ٣١٨، دما ٣١٦، الجنان ٢٠٥، مواليه ٣١٥<a href="#p12" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p13"></span>بشر بن أبى خازم الدبور ١١١، قروض ٦٨، جذام ٦٧<a href="#p13" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p14"></span>بكر بن النطاح الأغماد ٣١٢، النزف ٣٧٥<a href="#p14" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p15"></span><span class="c2">(ت)</span><a href="#p15" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p>
<p><span class="anchor" id="p16"></span>أبو تمام بالنافقاء ٣٨٦، سجراتى ٣٩١، مذهب ٣٨٤، طالبه ٤٠٦، ركوبا ٣٨٩، المغاربا ٤٢١، طالب ٣٨١، مغارب ٣٨٢ والعنب ٣٨٢، ٤٠١، ٤٠٢ الشباب ٣٩٦، كلب ٣٨٧، ٣٩٦، المكروب ٣٩٥، قاطب ٣٩٨، مناسب ٣٩٩، العطب ٤٠١، كالملاب ٤٠٤، مجيب ٤١٢، طالب ٤٢٢، أدبه ٤٠٩، والجثجاثا ٣٠٠، ٤٠١، المغيث ٤٠٩ والعهد ٣٩٧، ٤٠٣، لا أحد ٤٠٤، فوائده ٣٩٠، المعاد ٣، ٤١٣، الزند ٣٨٧، عبد الحميد ٣٨٨، الفؤاد ٤٠٧، الأجساد ٤٠٧، حسود ٤٢١، الوعد ٤٢٤، مرقد ٣٨٢، الوحاد ٣٨٣، الجلد ٣٨٨، المزبد ٣٩٢، العدد ٤٠٠، الدهر ٣٧٩، عذر ٣٨٠، البدر ٣٨١، ٤٠٢، ما يبصر ٣٩٠، وبر ٣٩٣، الدثار ٤٠٣، الرمس ٣٩٠، الكاس ٣٩١، الشمس ٣٩٦، إياس ٤٠٦، والباس ٤٠٦، أنسه ٣٩١، النفوس ٤٢٢، حضيض ٤١٤، يقع ٣٨٠، مضاجع ٣٨٣، يصرع ٤٠١، ومتالع ٤١٣، سبع ٤٢٢، الشجاع ٣٩٣،<a href="#p16" class="btn_tag btn btn-sm"><span class="text-gray fa fa-copy"></span></a></p> </div>